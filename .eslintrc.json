{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/recommended",
    "airbnb-base",
    "airbnb",
    "eslint:recommended",
    "prettier",
    "prettier/vue"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "indent": "error"
  }
}
